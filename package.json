{
  "name": "storeon-connect",
  "version": "1.0.0",
  "description": "",
  "main": "dist/es5.cjs.js",
  "module": "dist/es5.esm.js",
  "esmodule": "dist/esm.js",
  "source": "src/index.js",
  "exports": {
    ".": {
      "require": "./dist/es5.cjs.js",
      "default": "./dist/es5.esm.js"
    }
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build": "rollup -c",
    "size": "size-limit"
  },
  "dependencies": {
    "storeon": "^3.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@size-limit/preset-small-lib": "^4.12.0",
    "eslint": "^7.29.0",
    "rollup": "^2.52.1",
    "simple-git-hooks": "^2.4.1",
    "size-limit": "^4.12.0"
  },
  "sideEffects": false,
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run build && npm run size"
  },
  "size-limit": [
    {
      "path": "dist/es5.cjs.js",
      "limit": "359 B"
    },
    {
      "path": "dist/es5.esm.js",
      "limit": "366 B"
    },
    {
      "path": "dist/esm.js",
      "limit": "314 B"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/storeon-connect.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/storeon-connect/issues"
  },
  "keywords": [
    "storeon"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/storeon-connect#readme",
  "license": "MIT"
}
