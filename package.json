{
  "name": "storeon-connect",
  "version": "0.3.0",
  "description": "A tiny connector for observes changes in Storeon",
  "main": "dist/es5.cjs.js",
  "module": "dist/es5.esm.js",
  "esmodule": "dist/esm.js",
  "source": "src/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/es5.cjs.js",
      "default": "./dist/es5.esm.js"
    }
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build": "rollup -c",
    "size": "size-limit",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/jest": "^26.0.23",
    "eslint": "^7.29.0",
    "jest": "^27.0.4",
    "rollup": "^2.52.2",
    "simple-git-hooks": "^2.4.1",
    "size-limit": "^4.12.0",
    "storeon": "^3.1.4"
  },
  "peerDependencies": {
    "storeon": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "storeon": {
      "optional": true
    }
  },
  "sideEffects": false,
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run build && npm run size && npm test"
  },
  "size-limit": [
    {
      "path": "dist/es5.cjs.js",
      "limit": "209 B"
    },
    {
      "path": "dist/es5.esm.js",
      "limit": "215 B"
    },
    {
      "path": "dist/esm.js",
      "limit": "171 B"
    }
  ],
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/storeon-connect.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/storeon-connect/issues"
  },
  "keywords": [
    "storeon"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/storeon-connect#readme",
  "license": "MIT"
}
